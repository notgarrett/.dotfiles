[38;5;238m 342[0m   [38;5;238m│[0m [38;2;248;248;242m# options could need changes, related to used GPU and drivers.[0m
[38;5;238m 343[0m   [38;5;238m│[0m [38;2;248;248;242m# to find the right setting consult the archwiki or ask at the forum.[0m
[38;5;238m 344[0m   [38;5;238m│[0m [38;2;248;248;242m#[0m
[38;5;238m 345[0m   [38;5;238m│[0m [38;2;248;248;242m# xcompmgr: https://wiki.archlinux.org/title/Xcompmgr[0m
[38;5;238m 346[0m   [38;5;238m│[0m [38;2;248;248;242m# manpage: https://man.archlinux.org/man/xcompmgr.1.en[0m
[38;5;238m 347[0m   [38;5;238m│[0m [38;2;248;248;242m# install xcompmgr package to use it (yay -S xcompmgr)[0m
[38;5;238m 348[0m   [38;5;238m│[0m [38;2;248;248;242m#exec --no-startup-id xcompmgr -C -n &[0m
[38;5;238m 349[0m   [38;5;238m│[0m [38;2;248;248;242m# or an more specialized config like this:[0m
[38;5;238m 350[0m   [38;5;238m│[0m [38;2;248;248;242m#exec --no-startup-id xcompmgr -c -C -t-5 -l-5 -r4.2 -o.55 &[0m
[38;5;238m 351[0m   [38;5;238m│[0m [38;2;248;248;242m#[0m
[38;5;238m 352[0m   [38;5;238m│[0m [38;2;248;248;242m# or:[0m
[38;5;238m 353[0m   [38;5;238m│[0m [38;2;248;248;242m#[0m
[38;5;238m 354[0m   [38;5;238m│[0m [38;2;248;248;242m# picom: https://wiki.archlinux.org/title/Picom[0m
[38;5;238m 355[0m   [38;5;238m│[0m [38;2;248;248;242m# manpage: https://man.archlinux.org/man/picom.1.en[0m
[38;5;238m 356[0m   [38;5;238m│[0m [38;2;248;248;242m# The default configuration is available in /etc/xdg/picom.conf [0m
[38;5;238m 357[0m   [38;5;238m│[0m [38;2;248;248;242m# For modifications, it can be copied to ~/.config/picom/picom.conf or ~/.config/picom.conf[0m
[38;5;238m 358[0m   [38;5;238m│[0m [38;2;248;248;242m# install picom package (yay -S picom)[0m
[38;5;238m 359[0m   [38;5;238m│[0m [38;2;248;248;242m# start using default config [0m
[38;5;238m 360[0m   [38;5;238m│[0m [38;2;248;248;242m#exec_always --no-startup-id picom -b[0m
[38;5;238m 361[0m   [38;5;238m│[0m [38;2;248;248;242m#[0m
